; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ABERTRON"
run/main_scene="res://Scenes/Boot.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/show_image=false
config/icon="res://Assets/Sprites/AMBERTRON_LOGO.png"
config/windows_native_icon="res://Assets/Sprites/AMBERTRON_LOGO.ico"

[audio]

buses/default_bus_layout="res://Assets/Audio/AudioLayout.tres"
manager/sound/bus="Sound"
manager/sound/pool_1D_size=16
manager/sound/pool_2D_size=16
manager/sound/pool_3D_size=16
manager/sound/max_polyphony=32
manager/music/bus="Music"

[autoload]

GameManager="*res://Scenes/AutoLoad/game_manager.tscn"
FPS_HUD="*res://Scenes/AutoLoad/fps_hud.tscn"
SoundManager="*res://addons/resonate/sound_manager/sound_manager.gd"
MusicManager="*res://addons/resonate/music_manager/music_manager.gd"
DebugUI="*res://Scenes/AutoLoad/debug_ui.tscn"
TutorialGUI="*res://Scenes/AutoLoad/tutorial_gui.tscn"
PauseMenu="*res://Scenes/AutoLoad/pause_menu.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=4
window/size/resizable=false
window/size/extend_to_title=true
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/resonate/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "purple",
"res://Scenes/": "red",
"res://Scripts/": "blue"
}

[global_group]

Interactable="Areas that player can interact with manually"
CanBePickedUp=""
